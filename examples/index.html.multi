<script type="text/javascript">
    $(function () {
        $("#txt_search").keyup(function () {
            var searchid = $(this).val();
            var dataString = 'search=' + searchid;
//var dataString = 'search='+ searchid + '&userid=';

            alert("IN autosearch");

            if (searchid != '' && searchid.length >= 3) {
                $.ajax({
                    type: "POST",
                    url: 'https://www.multisoftsystems.com/index.php?ms=comn_pass&action=autosearch',
                    data: dataString,
                    cache: false,
                    success: function (data) {
                        $("#result").html(data).show();
                    }
                });
            }
            else {
                $("#result").hide();
            }


            return false;
        });

        jQuery("#result").on("click", function (e) {
            var $clicked = $(e.target);
            var $name = $clicked.find('.name_auto_s').html();
            var decoded = $("<div/>").html($name).text();
            $('#txt_search').val(decoded);

        });
        jQuery(document).on("click", function (e) {

            var $clicked = $(e.target);
            if (!$clicked.hasClass("show_auto_s_data")) {
                jQuery("#result").fadeOut();
            }
        });
        $('#txt_search').click(function () {
